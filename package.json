{
  "name": "ts-basic-configuration",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "prepare": "husky install",
    "start:dev": "ts-node-dev src/index.ts",
    "test": "jest --passWithNoTests --runInBand",
    "test:unit": "npm run test -- --watch",
    "test:unit:staged": "npm run test -- --findRelatedTests",
    "test:coverage": "npm run test -- --coverage",
    "test:mutation": "stryker run",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --ext .ts --fix",
    "lint:deadcode": "ts-prune -e",
    "style:check": "prettier --check src/**/*.ts",
    "style:fix": "prettier --write src/**/*.ts --log-level silent"
  },
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucsimao/my-basic-typescript-configuration.git"
  },
  "bugs": {
    "url": "https://github.com/lucsimao/my-basic-typescript-configuration/issues"
  },
  "homepage": "https://github.com/lucsimao/my-basic-typescript-configuration#readme",
  "description": "",
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "^3.682.0",
    "@aws-sdk/client-lambda": "^3.682.0",
    "@aws-sdk/client-sns": "^3.682.0",
    "@aws-sdk/client-sqs": "^3.682.0",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.13.0",
    "@stryker-mutator/core": "^8.6.0",
    "@stryker-mutator/jest-runner": "^8.6.0",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/parser": "^8.12.2",
    "eslint": "^9.13.0",
    "eslint-plugin-import-helpers": "^2.0.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^15.11.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "serverless": "^4.4.7",
    "serverless-offline": "^14.3.3",
    "ts-jest": "^29.2.5",
    "ts-node-dev": "^2.0.0",
    "ts-prune": "^0.10.3",
    "typescript": "^5.6.3",
    "@types/aws-lambda": "^8.10.145"
  }
